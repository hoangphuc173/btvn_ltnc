#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int n;
    cin>>n;
    int a[n];
    int b[n+1];
    int sum1=0;
    int sum2=0;
    for(int i=0;i<n;i++){
        cin>>a[i];
        sum1+=a[i];
    }
    for(int j=0;j<n+1;j++){
        cin>>b[j];
        sum2+=b[j];
    }
    int sum=sum2-sum1;
    cout<<sum;
}
