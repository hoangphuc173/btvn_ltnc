#include <iostream>
#include <string>
#include <bits//stdc++.h>
using namespace std;

int main() {
	// Complete the program
    string a,b;
    cin>>a;
    cin>>b;
    int x=a.size();
    int y=b.size();
    cout<< x<<" "<<y<<endl;
    cout<< a+b<< endl;
    swap(a[0],b[0]);
    cout<<a<<" "<<b;
  
    return 0;
}
